<script type="text/javascript">
	let campoSenha = document.querySelector('input[name="senha"]');
	let campoConfirmarSenha = document.querySelector('input[name="rsenha"]');
	let botao = document.querySelector('.frm-botao');
    let alerta = document.getElementById('alerta')

	campoSenha.addEventListener('input', function(){
		verificaCampos();
	});

	campoConfirmarSenha.addEventListener('input', function(){
		verificaCampos();
	});

	function verificaCampos() {
		if(campoSenha.value == campoConfirmarSenha.value && campoSenha.value.length >= 6){
            botao.disabled = false;
            alerta.classList.add('d-none');
            alerta.classList.remove('d-block');
        }
		else{
            botao.disabled = true;
            alerta.classList.add('d-block');
            alerta.classList.remove('d-none');
        }
	}

</script>
<script>
    $(document).ready(function(){
        $("#iTelefone").mask("(99) 9999-9999?9")
        .focusout(function (event) {  
            var target, phone, element;  
            target = (event.currentTarget) ? event.currentTarget : event.srcElement;  
            phone = target.value.replace(/\D/g, '');
            element = $(target);  
            element.unmask();  
            if(phone.length > 10) {  
                element.mask("(99) 99999-999?9");  
            } else {  
                element.mask("(99) 9999-9999?9");  
            }  
        });
    })
</script>
