<%-include('../cabecalhoadmin.ejs')%>
<div class="container py-5 text-justify">
    <h1 class="py-2 text-center">Adicionar Evento</h1>
    <form method="post">
        <div class="form-group">
            <label for="inputnome">Nome</label>
            <input type="text" name="nome" class="form-control" id="inputnome" placeholder="Digite seu nome.">
        </div>
        <div class="form-group">
            <label for="inputemail">Data</label>
            <input type="date" name="data" class="form-control" id="inputemail"
                placeholder="Digite o dia do Evento!">
        </div>

        <button type="submit" class="btn btn-primary mb-2">Enviar</button>
    </form>
</div>
